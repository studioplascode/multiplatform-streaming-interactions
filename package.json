{
  "name": "multiplatform-streaming-interactions",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv turbo run build --token=$TURBO_TOKEN",
    "dev": "dotenv turbo run dev --token=$TURBO_TOKEN",
    "lint": "dotenv turbo run lint --token=$TURBO_TOKEN",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "1.8.x"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.2"
}
